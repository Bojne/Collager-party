2\",\n      objects: [\n        { emoji: \"沚欺" },\n        { emoji: \"泓ｽ\" },\n        { emoji: \"沚､\" },\n        { emoji: \"洫夸沛ｻ窶構" },\n        { emoji: \"衍ｦ\" },\n        { emoji: \"泅\" },\n        { emoji: \"汳｣\" },\n        { emoji: \"汳蚕" },\n        { emoji: \"汳｡\" },\n        { emoji: \"沛窶構" },\n        { emoji: \"沛貼" },\n        { emoji: \"沍ｶ\" }\n      ]\n    }; // Emojis\n  }\n\n  render() {\n    // <Helmet bodyAttributes={{style: 'background-color : #fff'}}/>\n    const dragHandlers = { onStart: this.onStart, onStop: this.onStop };\n    // const style = { 'background-color': this.state.color };\n    // console.log(this.state.color)\n    document.body.style.backgroundColor = this.state.color;\n    console.log(this.state.color);\n    return (\n      <div >\n        <input\n          value={this.state.color}\n          onChange={evt => this.updateInputValue(evt)}\n        />\n        <div>\n          <Draggable\n            handle=\".handle\"\n            {...dragHandlers}\n          >\n            <div>\n              <img className=\"handle image\" src={img} alt=\"imgage\" />\n            </div>\n          </Draggable>\n        </div>\n\n        <div>\n          {this.state.objects.map(obj => {\n            return this.renderDraggable(obj.emoji);\n          })}\n        </div>\n      </div>\n    );\n  }\n\n  renderDraggable(emoji, position) {\n    return (\n      <Draggable\n        handle=\".handle\"\n        position={null}\n        scale={1}\n        onStart={this.handleStart}\n        onDrag={this.handleDrag}\n        onStop={this.handleStop}\n      >\n        <div>\n          <div className=\"handle\">\n            <div className=\"emoji\">{emoji}</div>\n          </div>\n        </div>\n      </Draggable>\n    );\n  }\n  updateInputValue(evt) {\n    this.setState({\n      color: evt.target.value\n    });\n  }\n}\n\nReactDOM.render(<App />, destination);\n"},"lineCount":null}},"error":null,"hash":"4a3d9c131b105e2c562ed5237a85b011","cacheData":{"env":{